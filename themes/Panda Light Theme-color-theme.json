{
	"name": "Panda Light Theme",
	"type": "light",
	"colors": {
		"editor.background": "#f0f0f0",
		"editor.foreground": "#000000",
		"editor.lineHighlightBackground": "#f5ecd5",
		"editor.selectionBackground": "#BFDBFE",
		"editor.selectionHighlightBackground": "#E0E0E0",
		"sideBar.background": "#f5f5f5",
		"editorLineNumber.foreground": "#9DA39A",
		"editorCursor.foreground": "#000000",
		"editor.findMatchBackground": "#FFBC5D",
		"editor.findMatchHighlightBackground": "#FFE9A6",
		"tab.activeForeground": "#000000",
		"tab.activeBackground": "#f0f0f0",
		"tab.inactiveBackground": "#b4b4b4",
		"tab.inactiveForeground": "#000000",
		"statusBar.background": "#c5c5c5",
		"statusBar.noFolderBackground": "#c5c5c5",
		"statusBar.debuggingBackground": "#c5c5c5",
		"statusBar.foreground": "#000000",
		"foreground": "#000000",
		"dropdown.background": "#d5d5d5",
		"dropdown.border": "#000000"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword.control",
				"storage"
			],
			"settings": {
				"foreground": "#000080",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#007100",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"background": "#DDDDFF",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Python: Docstring",
			"scope": "string.quoted.docstring.multi.python",
			"settings": {
				"foreground": "#808080",
				"fontStyle": ""
			}
		}
	]
}