{
	"name": "Panda Light Theme",
	"type": "light",
	"colors": {
		"editor.background": "#f0f0f0",
		"editor.foreground": "#000000ff",
		"editor.lineHighlightBackground": "#f5ecd5",
		"editor.selectionBackground": "#BFDBFE",
		"editor.selectionHighlightBackground": "#E0E0E0",
		"editorLineNumber.foreground": "#9DA39A",
		"editorCursor.foreground": "#000000",
		"editor.findMatchBackground": "#FFBC5D",
		"editor.findMatchHighlightBackground": "#FFE9A6",
		"statusBar.background": "#c5c5c5",
		"statusBar.noFolderBackground": "#c5c5c5",
		"statusBar.debuggingBackground": "#c5c5c5",
		"statusBar.foreground": "#505050",
		"foreground": "#000000",
		"sideBar.foreground": "#303030",
		"sideBar.background": "#f3f3f3",
		"gitDecoration.modifiedResourceForeground": "#008000",
		"gitDecoration.untrackedResourceForeground": "#ff0000",
		"editorSuggestWidget.background": "#f0f0f0",
		"sideBar.border": "#c5c5c5"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"comment.block.preprocessor",
				"comment.documentation",
				"comment.block.documentation",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword.control",
				"keyword.operator.logical",
				"storage"
			],
			"settings": {
				"foreground": "#000080",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#007100",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constant",
			"scope":"constant",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Constant: Character",
			"scope":"constant.character",
			"settings": {
				"foreground": "#007100"
			}
		},
		{
			"name": "Constant: Other",
			"scope":"constant.other",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Variable Parameter Function Call",
			"scope":"variable.parameter.function-call",
			"settings": {
				"foreground": "#660099"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"background": "#DDDDFF",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Python: Docstring",
			"scope": "string.quoted.docstring.multi.python",
			"settings": {
				"foreground": "#808080",
				"fontStyle": ""
			}
		},
		{
			"name": "Python: Magic Method",
			"scope":"support.function.magic.python",
			"settings": {
				"foreground": "#B200B2"
			}
		},
		{
			"name": "Python: Variable Self",
			"scope":"variable.language.special.self.python",
			"settings": {
				"foreground": "#94558D"
			}
		},
		{
			"name": "Python: Builtin Function",
			"scope": "support.function.builtin.python",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Python: Decorator",
			"scope": "meta.function.decorator.python",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "JSON: Type Name",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#660e7a"
			}
		}
	]
}